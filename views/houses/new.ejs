<%- include('../partials/header') %>
<form action="/houses" method="post">
  <label>Name: </label><input type="text" name="name"><p>
  <label>Location: </label><input type="text" name="location"><p>
  <label>Latitude/Longitude: </label><input type="text" name="latLon"><p>
  <label>Construction Year: </label><input type="number" name="constructionYear" min="1750" max="1900"><p>
  <label>Architectural Style: </label>
  <select name="archStyle">
    <option value="Federal">Federal</option>
    <option value="Colonial">Colonial</option>
    <option value="Other">Other</option>
  </select><p>
  <label>Featured Picture: </label><input type="text" name="featuredPicture"><p>
  <label>Description: </label><textarea name="description" rows="4" cols="60"></textarea><p>
  <input type="submit" value="Add House">  
</form>
<%- include('../partials/footer') %>
